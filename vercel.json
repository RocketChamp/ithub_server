{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/post/(.*)", "dest": "./routes/post.js" },
      { "src": "/user/(.*)", "dest": "./routes/user.js" },
      { "src": "/project/(.*)", "dest": "./routes/project.js" },
      { "src": "/(.*)", "dest": "./index.js" }
    ]
  }