{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/post/(.*)", "dest": "/post/$1" },
      { "src": "/user/(.*)", "dest": "/user/$1" },
      { "src": "/project/(.*)", "dest": "/project/$1" },
      { "src": "/(.*)", "dest": "./index.js" }
    ]
  }